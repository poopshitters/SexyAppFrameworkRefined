<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="audio/x-gme-spc700">
    <sub-class-of type="application/octet-stream"/>
    <comment>SPC700 memory dump with a music (SPC)</comment>
    <magic priority="50">
      <match type="string" offset="0" value="SNES"/>
    </magic>
    <glob pattern="*.spc"/>
  </mime-type>
</mime-info>
