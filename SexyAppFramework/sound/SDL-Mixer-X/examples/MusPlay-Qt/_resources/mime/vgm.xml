<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="audio/x-gme-vgm">
    <sub-class-of type="application/octet-stream"/>
    <comment>Video Game Music (VGM)</comment>
    <magic priority="50">
      <match type="string" offset="0" value="Vgm "/>
    </magic>
    <glob pattern="*.vgm"/>
  </mime-type>
</mime-info>
